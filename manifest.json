{
  "manifest_version": 2,

  "name": "Reddit Guns Chrome Extension",
  "description": "This extension shows a list of the most current Reddits on guns.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Reddit Guns!"
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
